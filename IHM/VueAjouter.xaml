<Window x:Class="IHM.VueAjouter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IHM"
        mc:Ignorable="d"
        Title="Ajouter" Height="300" Width="300">
    <Viewbox Stretch="Uniform">
        <Grid Height="269" Width="292">
            <Button x:Name="buttonValider" Content="Valider" HorizontalAlignment="Left" Margin="72,225,0,0" VerticalAlignment="Top" Width="75" Click="ClickValider"/>
            <Button x:Name="buttonAnnuler" Content="Annuler" HorizontalAlignment="Left" Margin="163,225,0,0" VerticalAlignment="Top" Width="75" Click="ClickAnnuler"/>
            <Label x:Name="labelNom" Content="Nom" HorizontalAlignment="Left" Margin="35,10,0,0" VerticalAlignment="Top"/>
            <Label x:Name="labelDescription" Content="Description" HorizontalAlignment="Left" Margin="35,65,0,0" VerticalAlignment="Top"/>
            <TextBox x:Name="textBoxNom" HorizontalAlignment="Left" Height="23" Margin="35,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
            <TextBox x:Name="textBoxDescription" HorizontalAlignment="Left" Height="94" Margin="35,96,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="225"/>

        </Grid>
    </Viewbox>
</Window>
